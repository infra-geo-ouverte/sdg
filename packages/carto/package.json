{
  "name": "@igo2/sdg-carto",
  "version": "1.0.0-next.91",
  "license": "LiLiQ-R",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/infra-geo-ouverte/sdg.git",
    "directory": "packages/carto"
  },
  "engines": {
    "node": ">=20.0.0"
  },
  "exports": {
    ".": {
      "import": "./src/public-api.ts"
    },
    "./ol": {
      "import": "./ol/src/index.ts"
    }
  },
  "sideEffects": false,
  "scripts": {
    "build": "ng build carto --configuration production",
    "postbuild": "node --import tsx ../../scripts/src/post-build.mts carto",
    "format": "prettier --check ./src/**/*.{ts,js,html,scss,css,json}",
    "format.fix": "prettier --write ./src/**/*.{ts,js,html,scss,css,json}",
    "lint": "ng lint carto",
    "lint.fix": "ng lint carto --fix",
    "test": "ng test carto --watch=false",
    "test.watch": "ng test carto",
    "watch": "ng build carto --watch && npm run postbuild"
  },
  "dependencies": {
    "tslib": "^2.6.0"
  },
  "peerDependencies": {
    "@angular/common": "^19.2.0",
    "@angular/core": "^19.2.0",
    "@angular/material": "^19.2.0",
    "@angular/platform-browser": "^19.2.0",
    "@igo2/sdg-core": "*",
    "@igo2/utils": "^19.0.0-next",
    "ol": "^10.5.0",
    "rxjs": "~7.8.0"
  },
  "devDependencies": {
    "typescript": "~5.5.4"
  }
}
